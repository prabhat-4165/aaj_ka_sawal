// move zeroes to the end 
void pushZerosToEnd(int A[], int N) {
	    int i=0;
        for(int j=0;j<N;j++){
            if (A[j] != 0){
                swap(A[j],A[i]);
                i++;
            }
        }
	}
  
  // find the unique element
  int search(int A[], int N){
	    int ans = 0;
	    for (int i=0;i<N;i++)
	    {
	        ans = ans^A[i];
	    }
	    return ans;
	}
